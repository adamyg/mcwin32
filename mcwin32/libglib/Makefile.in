@SET_MAKE@
ROOT=		@abs_top_builddir@
top_builddir=	@top_builddir@

# File extensions

E=
O=		.o
H=		.h
A=		.a
LP=		lib

CLEAN=		*.bak *~ *.BAK *.swp *.tmp core *.core a.out
XCLEAN=

# Directories

D_BIN=		$(ROOT)/bin@TOOLCHAINEXT@
D_INC=		$(ROOT)/include
D_OBJ=		$(ROOT)/objects@TOOLCHAINEXT@/libglib
D_LIB=		$(ROOT)/lib@TOOLCHAINEXT@

# Compilers, programs

CC=		@CC@
AR=		@AR@
RANLIB=		@RANLIB@
RM=		@RM@
CP=		@CP@
PERL=		@PERL@
LIBTOOL=	@LIBTOOL@
RC=		@RC@

# Common flags

ARFLAGS=	rcv
XFLAGS=		
COPT=		@CFLAGS@
CXXOPT=		@CXXFLAGS@
ifeq "" "$(CXXOPT)"
CXXOPT=		$(COPT)
endif
CWARN=		@CWARN@
CDEBUG=		@CDEBUG@
LDEBUG=		@LDEBUG@
CINCLUDE=	-I. -I$(D_INC)
CEXTRA=		@DEFS@
CFLAGS=		$(CPPFLAGS) $(CDEBUG) $(COPT) $(CWARN) $(CEXTRA) $(CINCLUDE) $(XFLAGS)
CXXFLAGS=	$(CPPFLAGS) $(CDEBUG) $(CXXOPT) $(CWARN) $(CEXTRA) $(CINCLUDE) $(XFLAGS)
YFLAGS=		-d
LDFLAGS=	$(LDEBUG) @LDFLAGS@
LDLIBS=		$(D_LIB)/$(LP)intl$(A) winmm.lib @EXTRALIBS@

RMFLAGS=	-f

############################################################

# Targets

GLIBLIB=	$(D_LIB)/$(LP)glib-2.0_static$(A)
GLIBDLL=	$(D_LIB)/$(LP)glib-2.0.la

GMODULELIB=	$(D_LIB)/$(LP)gmodule-2.0_static$(A)
GMODULEDLL=	$(D_LIB)/$(LP)gmodule-2.0.la

VERSION=	2.51.4
VERSIONSPEC=	$(subst .,:,$(VERSION))
VERPACKED=	glib-$(VERSION).tgz

GLIBBASE=	./glib-$(VERSION)
GLIBSRC=	$(GLIBBASE)/glib
GLIBINCLUDE=	$(GLIBBASE)/glib
GMODULESRC=	$(GLIBBASE)/gmodule

CINCLUDE+=	-I$(GLIBSRC) -I$(GLIBBASE) -I./sdk
CEXTRA+=	-D_WIN32_WINNT=0x501 -DGLIB_COMPILATION -D_WIN32

###	glib

#Note: sourced from glib.vcproj, see $(GLIBSRC)/win32
CEXTRA_LIBCHARSET = \
	-DLIBDIR="blah"

CEXTRA_PCRE = \
	-DHAVE_CONFIG_H \
	-DGPCRE_COMPILATION \
	-DLINK_SIZE=2 \
	-DMAX_NAME_SIZE=32 \
	-DMAX_NAME_COUNT=10000 \
	-DPOSIX_MALLOC_THRESHOLD=10 \
	-DMATCH_LIMIT=10000000 \
	-DMATCH_LIMIT_RECURSION=10000000 \
	-DSUPPORT_UCP \
	-DSUPPORT_UTF \
	-DSUPPORT_UTF8 \
	-DG_LOG_DOMAIN="GLib-GRegex" \
	-UBSR_ANYCRLF \
	-UEBCDIC
##	-DPCRE_STATIC	##global
##	-DNEWLINE=-1	#see config.h

CEXTRA_GTHREAD = \
	-DG_LOG_DOMAIN="GThread"
	
CEXTRA_GMODULE = \
	-DG_LOG_DOMAIN="GModule"

CEXTRA_GIO = \
	-DGIO_COMPILATION \
	-DG_LOG_DOMAIN="GLib-GIO"

CEXTRA_GOBJECT = \
	-DGOBJECT_COMPILATION \
	-DG_LOG_DOMAIN="GLib-GObject" \
	-DFFI_BUILDING

VPATH+=		$(GLIBSRC)
base=\
	garray.c \
	gasyncqueue.c \
	gatomic.c \
	gbacktrace.c \
	gbase64.c \
	gbitlock.c \
	gbookmarkfile.c \
	gbytes.c \
	gcharset.c \
	gchecksum.c \
	gconvert.c \
	gdataset.c \
	gdate.c \
	gdatetime.c \
	gdir.c \
	genviron.c \
	gerror.c \
	gfileutils.c \
	ggettext.c \
	ghash.c \
	ghmac.c \
	ghook.c \
	ghostutils.c \
	giochannel.c \
	giowin32.c \
	gkeyfile.c \
	glib-init.c \
	glib-private.c \
	glist.c \
	gmain.c \
	gmappedfile.c \
	gmarkup.c \
	gmem.c \
	gmessages.c \
	gnode.c \
	goption.c \
	gpattern.c \
	gpoll.c \
	gprimes.c \
	gprintf.c \
	gqsort.c \
	gquark.c \
	gqueue.c \
	grand.c \
	gregex.c \
	gscanner.c \
	gsequence.c \
	gshell.c \
	gslice.c \
	gslist.c \
	gspawn-win32.c \
	gstdio.c \
	gstrfuncs.c \
	gstring.c \
	gstringchunk.c \
	gtestutils.c \
	gthread-win32.c \
	gthread.c \
	gthreadpool.c \
	gtimer.c \
	gtimezone.c \
	gtranslit.c \
	gtrashstack.c \
	gtree.c \
	gunibreak.c \
	gunicollate.c \
	gunidecomp.c \
	guniprop.c \
	gurifuncs.c \
	gutf8.c \
	gutils.c \
	guuid.c \
	gvariant-core.c \
	gvariant-parser.c \
	gvariant-serialiser.c \
	gvariant.c \
	gvarianttype.c \
	gvarianttypeinfo.c \
	gversion.c \
	gwakeup.c \
	gwin32.c

deprecated=\
	gallocator.c \
	gcache.c \
	gcompletion.c \
	grel.c \
	gthread-deprecated.c

libcharset=\
	localcharset.c

gnulib=\
	asnprintf.c \
	printf-args.c \
	printf-parse.c \
	printf.c \
	vasnprintf.c

pcre=\
	pcre_byte_order.c \
	pcre_chartables.c \
	pcre_compile.c \
	pcre_config.c \
	pcre_dfa_exec.c \
	pcre_exec.c \
	pcre_fullinfo.c \
	pcre_get.c \
	pcre_globals.c \
	pcre_jit_compile.c \
	pcre_newline.c \
	pcre_ord2utf8.c \
	pcre_string_utils.c \
	pcre_study.c \
	pcre_tables.c \
	pcre_valid_utf8.c \
	pcre_version.c \
	pcre_xclass.c

GLIBSOURCES=\
	$(base) \
	$(deprecated) \
	$(libcharset) \
	$(gnulib) \
	$(pcre)

GLIBLIBOBJS=\
	$(addprefix $(D_OBJ)/,$(subst .c,$(O),$(GLIBSOURCES)))

GLIBDLLOBJS=\
	$(subst $(O),.lo,$(GLIBLIBOBJS))
ifeq ("mingw32","@build_os@")
GLIBDLLOBJS+=	$(D_OBJ)/glib_res.o
else ifeq ("win32","@build_os@")
GLIBDLLOBJS+=	$(D_OBJ)/glib.res
endif

###	gmodule

#Note: sourced from gmodule.vcproj, see $(GLIBSRC)/win32

VPATH+=		$(GMODULESRC)
GMODULESOURCES=\
	gmodule.c

GMODULELIBOBJS=\
	$(addprefix $(D_OBJ)/,$(subst .c,$(O),$(GMODULESOURCES)))

GMODULEDLLOBJS=\
	$(subst $(O),.lo,$(GMODULELIBOBJS))
ifeq ("mingw32","@build_os@")
DLLOBJS+=	$(D_OBJ)/gmodule_res.o
else ifeq ("win32","@build_os@")
DLLOBJS+=	$(D_OBJ)/gmodule.res
endif

###

OBJS=		$(GBLIBLIBOBJS) $(GLIBDLLOBJS) \
		$(GMODULELIBOBJS) $(GMODULEDLLOBJS)

LIBS=		$(GLIBLIB) $(GLIBDLL) \
		$(GMODULELIB) $(GMODULEDLL)
TSKS=

############################################################
# Rules

all:			source unpacked
unpacked:		object $(LIBS) $(TSKS) installheaders

$(GLIBLIB):		CEXTRA += -DGLIB_STATIC_COMPILATION
$(GLIBLIB):		$(GLIBLIBOBJS)
		$(RM) $(RMFLAGS) $@ >/dev/null 2>&1
		$(AR) $(ARFLAGS) $@ $^
		$(RANLIB) $@

$(GLIBDLL):		CEXTRA += -DGLIB_DYNAMIC_COMPILATION -DDLL_EXPORT -DPCRE_STATIC
$(GLIBDLL):		$(GLIBDLLOBJS)
		$(LIBTOOL) --mode=link $(CC) $(LDFLAGS) -version-number $(VERSIONSPEC) \
			-rpath $(D_LIB) -bindir $(D_BIN) -o $@ $^ $(LDLIBS)

$(GMODULELIB):		CEXTRA += -DGMODULE_STATIC_COMPILATION
$(GMODULELIB):		$(GMODULELIBOBJS)
		$(RM) $(RMFLAGS) $@ >/dev/null 2>&1
		$(AR) $(ARFLAGS) $@ $^
		$(RANLIB) $@

$(GMODULEDLL):		CEXTRA += -DGMODULE_DYNAMIC_COMPILATION -DDLL_EXPORT
$(GMODULEDLL):		$(GMODULEDLLOBJS)
		$(LIBTOOL) --mode=link $(CC) $(LDFLAGS) -version-number $(VERSIONSPEC) \
			-rpath $(D_LIB) -bindir $(D_BIN) -o $@ $^ $(D_LIB)/$(LP)glib-2.0.lib $(LDLIBS)

source:			.unpacked.$(VERSION)
		$(MAKE) unpacked

.unpacked.$(VERSION):	$(VERPACKED)
		@echo "do not delete, managed file" > $@
		@echo "unpacking source $VERPACKED"
		@gzip -c -d $(VERPACKED) | tar -xvf -

.PHONY:		object
object:			$(D_OBJ)/.created

.PHONY:		installheaders
installheaders:		Makefile ../include/.created \
				../include/glib-2.0/.created \
				../include/glib-2.0/glib/.created \
				../include/glib-2.0/glib/deprecated/.created
		@echo publishing glib headers ...
		@$(CP) $(GLIBINCLUDE)/glib.h			../include/glib-2.0
		@$(CP) $(GLIBINCLUDE)/glibconfig.h.win32	../include/glib-2.0/glibconfig.h
		@$(CP) $(GLIBINCLUDE)/galloca.h			../include/glib-2.0/glib
