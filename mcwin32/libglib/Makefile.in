		@$(CP) $(GLIBINCLUDE)/gconvert.h		../include/glib-2.0/glib
		@$(CP) $(GLIBINCLUDE)/gdataset.h		../include/glib-2.0/glib
		@$(CP) $(GLIBINCLUDE)/gdate.h			../include/glib-2.0/glib
		@$(CP) $(GLIBINCLUDE)/gdatetime.h		../include/glib-2.0/glib
